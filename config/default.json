{
	"support": {
		"explorer": "9",
		"firefox": 15,
		"chrome": 25,
		"safari": "5",
		"opera": 12,
		"android": 4,
		"ios": 5
	},
	"plugins": [
		"calc",
		"cursor",
		"fullscreen"
	],
	"code": {
		"indent": "\t",
		"linebreak": "\n",
		"selectorJoiner": ", ",
		"argumentJoiner": ", ",
		"valueJoiner": ", ",
		"ruleColon": ": ",
		"ruleEnd": ";",
		"comments": "all",
		"commentStart": "/* ",
		"commentEnd": " */",
		"rulesetStart": " {\n",
		"rulesetEnd": "\n}"
	}
}
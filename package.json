{
	"name": "stylecow",
	"description": "Modern CSS for all browsers",
	"version": "1.2.0-rc1",
	"author": "Oscar Otero <oom@oscarotero.com>",
	"homepage": "https://github.com/stylecow/stylecow",
	"bugs": "https://github.com/stylecow/stylecow/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/stylecow/stylecow.git"
	},
	"dependencies": {
		"commander": "*",
		"inquirer": "*",
		"glob": "*"
	},
	"main": "./lib/index",
	"bin": {
		"stylecow": "./bin/stylecow"
	},
	"license": "MIT",
	"jshintConfig": {
		"node": true,
		"bitwise": true,
		"camelcase": false,
		"curly": true,
		"eqeqeq": true,
		"forin": false,
		"immed": true,
		"latedef": true,
		"newcap": true,
		"noarg": true,
		"noempty": true,
		"nonew": true,
		"onevar": true,
		"plusplus": false,
		"quotmark": "simple",
		"regexp": true,
		"strict": false,
		"trailing": true,
		"undef": true,
		"unused": "vars",
		"globals": {
			"after": false,
			"afterEach": false,
			"before": false,
			"beforeEach": false,
			"describe": false,
			"it": false,
			"xdescribe": false,
			"xit": false
		}
	}
}
